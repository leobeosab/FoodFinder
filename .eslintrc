{
  "globals": {
    "navigator": false,
    "test": false,
    "fetch": false,
  },
  "extends": "eslint-config-airbnb",
  "rules": {
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  "no-trailing-spaces": "off",
  "import/extensions": "off",
  "no-param-reassign": "off",
  "react/forbid-prop-types": "off",
  "max-len": "off",
  }
}
